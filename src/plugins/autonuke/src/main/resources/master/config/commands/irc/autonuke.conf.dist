autonukes {
plugin org.drftpd.autonuke.master
class AutoNukeCommands
method doAUTONUKES
perms =siteop
help Show AutoNuke queue.
help.specific Show AutoNuke queue.
help.specific ${command} <section>
input all
output source
}

autonukescan {
plugin org.drftpd.autonuke.master
class AutoNukeCommands
method doAUTONUKESCAN
perms =siteop
help Manual AutoNuke scan.
help.specific Manual AutoNuke scan.
help.specific ${command} <path>
input all
output source
}

delqueue {
plugin org.drftpd.autonuke.master
class AutoNukeCommands
method doDELQUEUE
perms =siteop
help Delete one/all item(s) from AutoNuke queue.
help.specific Delete one/all item(s) from AutoNuke queue.
help.specific ${command} <path>
input all
output source
}
